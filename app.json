{
  "expo": {
    "name": "Finbel",
    "slug": "fm_mobile",
    "version": "3.1.0",
    "orientation": "portrait",
    "icon": "./images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./images/logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.avdias.fm_mobile"
    },
    "androidNavigationBar": {
      "backgroundColor": "#12102E"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0d93d640-abb9-49dd-a31c-eee72b033d17"
      }
    },
    "plugins": [
      "expo-secure-store",
       [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "gradleVersion": "8.6.0",
            "gradlePluginVersion": "8.6.0"
          }
        }
      ]
    ]
  }
}
